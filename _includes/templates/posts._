<div class='inner'>
  <div class='breadcrumb'>
    <a class='branch' href='#<%= [user, repo, "tree", branch].join("/") %>'><%= branch %></a>
    <% _.each(_.chunkedPath(path), function(p) { %>
      <span class='slash'>/</span>
      <a class='path' href='#<%= [user, repo, "tree", branch, p.url].join("/") %>'><%= p.name %></a>
    <% }); %>
  </div>
  <input placeholder='Filter Files' type='text' id='search_str' />
</div>
<ul id='files' class='listing'></ul>
