<!-- Publish Status -->
<div class='status'></div>

<div class='inner'>
  <div class='diff'></div>
  <div class='surface<%= preview ? " preview" : " compose" %>'>
      <div id='code'></div>
      <div class='content-preview prose'>
        <div class='post-content'>
          <%= markdown ? marked(content) : '' %>
        </div>
    </div>
  </div>
</div>
