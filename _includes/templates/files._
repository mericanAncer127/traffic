<% _.each(files, function(f) { %>
  <% if (f.type === 'tree') {Â %>
    <!-- folders -->
    <li><a class='link folder' href='#<%= user %>/<%= repo %>/tree/<%= branch %><%= f.path ? "/" +f.path : "" %>'>
      <%= f.path === _.parentPath(current_path) ? ".." : f.name %>
    </a></li>
  <% } else { %> 
    <!-- files -->
    <li><a class='link load-post toggle-view' href='#<%= user %>/<%= repo %>/edit/<%= branch %>/<%= f.path %>'>
      <%= f.name || 'Untitled' %>
    </a></li>
  <% } %>
<% }); %>
