<div class='profile-header'>


  

  <img class='avatar' src='<%= user.avatar_url %>' width='80' height='80'/>
  <h2><a href='http://github.com/<%= user.login %>' target='_blank'><%= user.name ||Â user.login %></a></h2>
</div>

<div class='content-search'>
  <span class='icon search inline fr'></span>
  <input type='text' placeholder='Filter Projects' />
</div>

<ul class='listing'>
  <% _.each(repos, function(r) { %>
    <li><a title='edit' class='select-repo repo' data-user='<%= r.owner.login %>' data-repo='<%= r.name %>' href="#<%= r.owner.login %>/<%= r.name %>">
      <%= r.name %>
      <span class='deemphasize'><%= r.description %></span>
    </a></li>
  <% }); %>
</ul>
